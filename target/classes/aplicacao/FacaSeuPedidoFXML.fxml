<AnchorPane prefHeight="285.0" prefWidth="456.0" styleClass="pane"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="aplicacao.controllers.ControllerFacaSeuPedido">
	<children>
		<BorderPane centerShape="true" layoutY="-1.0" minHeight="500"
			minWidth="500" prefHeight="501.0" prefWidth="500.0" styleClass="pane">
			<center>
				<VBox alignment="CENTER" prefHeight="200.0" prefWidth="240.0"
					spacing="40" BorderPane.alignment="CENTER">
					<children>
						<Label fx:id="labelPrincipal" alignment="CENTER"
							contentDisplay="CENTER" layoutX="70.0" layoutY="14.0"
							text="Seja bem-vindo, em que posicao este dispositivo pertence ?" />

						<Button mnemonicParsing="false" onAction="#onAtendimento"
							prefHeight="36.0" prefWidth="113.0" text="Atendimento">
							<graphic>
								<ImageView fitHeight="25.0" fitWidth="21.0">
									<image>
										<Image url="/resources/images/write.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
						<Button mnemonicParsing="false" onAction="#onCaixa"
							prefHeight="23.0" prefWidth="113.0" text="Caixa           ">
							<graphic>
								<ImageView fitHeight="24.0" fitWidth="25.0">
									<image>
										<Image url="/resources/images/server.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
						<Button mnemonicParsing="false" onAction="#onCozinha"
							prefHeight="31.0" prefWidth="116.0" text="Cozinha       ">
							<graphic>
								<ImageView fitHeight="23.0" fitWidth="29.0">
									<image>
										<Image url="/resources/images/chef-hat.png" />
									</image>
								</ImageView>
							</graphic>
						</Button>
					</children>

				</VBox>
			</center>
		</BorderPane>
	</children>
</AnchorPane>
