									<children>
										<Label text="Mesa" />
										<Button mnemonicParsing="false" onAction="#onItensMesa"
											prefHeight="25.0" prefWidth="51.0" styleClass="button" text="Itens"
											GridPane.rowIndex="2" />
										<Button fx:id="btnAlteracaoMesa" mnemonicParsing="false"
											onAction="#enviarPedidoMesa" styleClass="button" text="Pronto"
											GridPane.columnIndex="1" GridPane.rowIndex="2" />
										<ComboBox fx:id="comboMesa" prefWidth="150.0"
											styleClass="comboBox" GridPane.columnIndex="1" />
										<TableView fx:id="tableMesa" prefHeight="300.0"
											prefWidth="300.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
											<columns>
												<TableColumn prefWidth="75.0" text="preco">
													<cellValueFactory>
														<PropertyValueFactory property="preco" />
													</cellValueFactory>
												</TableColumn>
												<TableColumn prefWidth="42.0" text="unid">
													<cellValueFactory>
														<PropertyValueFactory property="unid" />
													</cellValueFactory>
												</TableColumn>
												<TableColumn prefWidth="127.0" text="nome">
													<cellValueFactory>
														<PropertyValueFactory property="nome" />
													</cellValueFactory>
												</TableColumn>
												<TableColumn prefWidth="56.0" text="item 1" />
												<TableColumn prefWidth="75.0" text="item 2" />
												<TableColumn prefWidth="75.0" text="item 3" />
												<TableColumn prefWidth="75.0" text="item 4" />
											</columns>
										</TableView>
										<Label fx:id="precoMesa" alignment="CENTER"
											contentDisplay="CENTER" prefHeight="17.0" prefWidth="62.0"
											text="0.0" GridPane.rowIndex="1" />
									</children>
								</GridPane>
							</children>
						</AnchorPane>
					</content>
				</Tab>
			</tabs>
